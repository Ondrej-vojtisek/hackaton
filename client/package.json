{
  "name": "finbricks_hackathon",
  "version": "1.0.0",
  "author": "Morosystems s.r.o.",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "start": "webpack-dev-server --open --define-process-env-node-env development",
    "build": "npm run pre-build-check && webpack --define-process-env-node-env production",
    "build:dev": "npm run pre-build-check && webpack --define-process-env-node-env development",
    "pre-build-check": "run-p type-check cycle-check lint",
    "type-check": "tsc --noEmit",
    "cycle-check": "dpdm ./src --warning false --tree false --exit-code circular:1",
    "lint": "eslint ./src --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:fix": "npm run lint -- --fix",
    "lint:nocache": "eslint ./src",
    "generate-types:local": "run-s generate-types:download:local generate-types:postProcess",
    "generate-types:postProcess": "run-s generate-types:prettier generate-types:copy:dto generate-types:clear",
    "generate-types:download": "openapi-generator-cli generate --generator-key typescript-fetch-dev",
    "generate-types:download:local": "openapi-generator-cli generate --generator-key typescript-fetch-local",
    "generate-types:prettier": "prettier --write \"openapi/models/*.ts\"",
    "generate-types:copy:dto": "copyfiles -f --verbose \"openapi/models/*.ts\" src/types/be/generated",
    "generate-types:clear": "rimraf -r openapi"
  },
  "dependencies": {
    "@babel/runtime": "7.20.7",
    "@emotion/react": "11.10.6",
    "@mantine/core": "6.0.8",
    "@mantine/hooks": "6.0.8",
    "@mantine/modals": "6.0.8",
    "@mantine/notifications": "6.0.8",
    "@reduxjs/toolkit": "1.9.1",
    "@tabler/icons-react": "2.17.0",
    "core-js": "3.27.1",
    "date-fns": "2.29.3",
    "i18next": "22.4.7",
    "immer": "9.0.16",
    "lorem-ipsum": "2.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.3.0",
    "react-i18next": "12.1.1",
    "react-redux": "8.0.5",
    "react-router-dom": "6.6.1",
    "redux": "4.2.0",
    "regenerator-runtime": "0.13.11",
    "reselect": "4.1.7",
    "styled-components": "5.3.6",
    "victory": "36.6.8"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-redux": "7.1.25",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "babel-loader": "9.1.0",
    "copyfiles": "2.4.1",
    "dotenv-webpack": "8.0.1",
    "dpdm": "3.10.0",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-yml": "1.4.0",
    "git-revision-webpack-plugin": "5.0.0",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "type-fest": "3.5.0",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "yaml-eslint-parser": "1.1.0",
    "yaml-loader": "0.8.0"
  }
}
