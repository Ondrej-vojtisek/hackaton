# Editor configuration, see http://editorconfig.org. Documentation of this configuration is in src/documentation/infrastructure/codeStyle.story.mdx. Keep in sync.
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[package.json]
indent_size = 2
